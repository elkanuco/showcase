@prefix cv: <http://rdfs.org/resume-rdf/cv.rdfs#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Main CV instance
<http://example.org/cv/elon-musk>
    a cv:CV ;
    cv:aboutPerson <http://example.org/person/elon-musk> ;
    cv:cvTitle "Curriculum Vitae of Elon Musk" ;
    cv:cvDescription "Comprehensive resume detailing professional experience, education, and skills of Elon Musk, entrepreneur and CEO of Tesla, SpaceX, and other ventures." ;
    cv:cvIsActive "true"^^xsd:boolean ;
    cv:cvIsConfidential "false"^^xsd:boolean ;
    cv:cvCopyright "Copyright 2025 Elon Musk. All rights reserved." ;
    cv:lastUpdate "2025-09-17"^^xsd:date ;
    cv:hasWorkHistory <http://example.org/workhistory/tesla> , <http://example.org/workhistory/spacex> , <http://example.org/workhistory/xai> , <http://example.org/workhistory/doge> ;
    cv:hasEducation <http://example.org/education/upenn> , <http://example.org/education/preston> ;
    cv:hasSkill <http://example.org/skill/programming> , <http://example.org/skill/engineering> , <http://example.org/skill/leadership> , <http://example.org/skill/languages> ;
    cv:hasTarget <http://example.org/target/job> ;
    cv:hasOtherInfo <http://example.org/otherinfo/philanthropy> .

# Person details
<http://example.org/person/elon-musk>
    a foaf:Person ;
    foaf:name "Elon Reeve Musk" ;
    cv:birthPlace "Pretoria, South Africa" ;
    cv:gender "male" ;
    cv:hasNationality "South African, Canadian, American" ;
    cv:hasCitizenship <http://www.daml.org/2001/09/countries/countries.daml#ZAF> , <http://www.daml.org/2001/09/countries/countries.daml#CAN> , <http://www.daml.org/2001/09/countries/countries.daml#USA> ;
    cv:maritalStatus "divorced" ;
    cv:noOfChildren "14" ;
    cv:hasDriversLicense "true"^^xsd:boolean .

# Work History: Tesla
<http://example.org/workhistory/tesla>
    a cv:WorkHistory ;
    cv:employedIn <http://example.org/company/tesla> ;
    cv:startDate "2004"^^xsd:gYear ;
    cv:endDate "" ;  # Ongoing
    cv:isCurrent "true"^^xsd:boolean ;
    cv:jobTitle "CEO and Product Architect" ;
    cv:jobDescription "Joined Tesla as Chairman and led the company as CEO since 2008. Oversaw the development of electric vehicles like the Model S, Model 3, Cybertruck, and advancements in autonomous driving technology. Expanded Tesla into energy storage with Powerwall and solar products." ;
    cv:jobType "employee" ;
    cv:careerLevel "executive" ;
    cv:numSubordinates "thousands" .

# Company: Tesla
<http://example.org/company/tesla>
    a cv:Company ;
    cv:Name "Tesla, Inc." ;
    cv:URL "https://www.tesla.com" ;
    cv:Country <http://www.daml.org/2001/09/countries/countries.daml#USA> ;
    cv:Locality "Austin, Texas" ;
    cv:Industry "Automotive / Electric Vehicles" ;
    cv:Notes "Leading manufacturer of electric vehicles and sustainable energy solutions." .

# Work History: SpaceX
<http://example.org/workhistory/spacex>
    a cv:WorkHistory ;
    cv:employedIn <http://example.org/company/spacex> ;
    cv:startDate "2002"^^xsd:gYear ;
    cv:endDate "" ;  # Ongoing
    cv:isCurrent "true"^^xsd:boolean ;
    cv:jobTitle "Founder, CEO, and Chief Engineer" ;
    cv:jobDescription "Founded SpaceX to reduce space transportation costs and enable Mars colonization. Developed reusable rockets like Falcon 9 and Starship. Launched Starlink constellation with over 7,600 satellites as of 2025. Achieved milestones including crewed missions to the ISS and first private company to send astronauts to space." ;
    cv:jobType "employee" ;
    cv:careerLevel "executive" ;
    cv:numSubordinates "thousands" .

